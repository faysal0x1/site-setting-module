{
    "name": "faysal0x1/site-setting-module",
    "description": "Site setting module (business info, social links, etc.) for Laravel.",
    "type": "laravel-module",
    "version": "1.0.0",
    "license": "MIT",
    "keywords": [
        "laravel",
        "module",
        "site-setting"
    ],
    "authors": [
        {
            "name": "Faysal",
            "homepage": "https://github.com/faysal0x1"
        }
    ],
    "require": {
        "php": "^8.2",
        "nwidart/laravel-modules": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Modules\\SiteSetting\\": ""
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Modules\\SiteSetting\\Tests\\": "tests/"
        }
    },
    "extra": {
        "module-dir": "Modules"
    }
}